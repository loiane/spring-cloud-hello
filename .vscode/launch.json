      "name": "Launch UserServiceApplication",
      "request": "launch",
      "mainClass": "com.loiane.userservice.UserServiceApplication"
    },
    {
      "type": "java",
      "name": "Spring Boot-DiscoveryServiceApplication<discovery-service>",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "mainClass": "com.loiane.discoveryservice.DiscoveryServiceApplication",
      "projectName": "discovery-service",
      "args": "",
      "envFile": "${workspaceFolder}/.env",
